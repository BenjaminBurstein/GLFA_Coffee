      "type": "integer",
      "required": true,
      "min": 1,
      "default": 1
    },
    "sale_date": {
      "type": "datetime",
      "required": true,
      "default": "2023-12-31T23:00:00.000Z"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
